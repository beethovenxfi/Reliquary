[profile.default]
src = 'contracts'
out = 'out'
libs = ['lib']
test = 'test/foundry'
cache_path = 'forge-cache'
script = 'scripts'
#solc = "0.8.13"
optimizer = true
optimizer_runs = 200
via_ir = true
verbosity = 1
#eth-rpc-url = "https://rpcapi.fantom.network"
#rpc_endpoints = { goerli = "https://eth-goerli.alchemyapi.io/v2/${ALCHEMY_API_KEY}" }
[rpc_endpoints]
goerli = "${GOERLI_RPC_URL}"
#sender = ''
#initial_balance = '0xffffffffffffffffffffffff'
ffi = false
#invariant_fail_on_revert = true
[etherscan]
goerli = { key = "${ETHERSCAN_KEY}" }

# See more config options https://github.com/foundry-rs/foundry/tree/master/config
